    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-hydrogen": "0.12.2",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.5.14",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
