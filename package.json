    "react-intersection-observer": "^9.10.3",
    "react-player": "^2.16.0",
    "react-use": "^17.5.0",
    "schema-dts": "^1.1.2",
    "swiper": "^11.1.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tiny-invariant": "^1.3.3",
    "typographic-base": "^1.0.4"
