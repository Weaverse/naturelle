{
  "name": "naturelle",
  "private": true,
  "sideEffects": false,
  "version": "2.0.0",
  "scripts": {
    "build": "shopify hydrogen build --codegen",
    "dev": "shopify hydrogen dev --port 3456 --codegen",
    "preview": "npm run build && shopify hydrogen preview",
